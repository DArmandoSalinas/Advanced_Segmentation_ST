{
  "version": 2,
  "builds": [
    {
      "src": "app/streamlit_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/streamlit_app.py"
    }
  ],
  "functions": {
    "app/streamlit_app.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "STREAMLIT_SERVER_PORT": "8501",
    "STREAMLIT_SERVER_ADDRESS": "0.0.0.0",
    "STREAMLIT_BROWSER_GATHER_USAGE_STATS": "false"
  }
}